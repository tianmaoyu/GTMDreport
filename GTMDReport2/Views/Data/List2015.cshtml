@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
@Styles.Render("~/Content/css")
<link rel="stylesheet" href="~/Content/ThemesBusiness/Home.css" />
@Styles.Render("~/Content/ThemesEasyui/gray/easyui")
@Styles.Render("~/Content/ThemesEasyui/red/exeasyui")
<link rel="stylesheet" href="~/Content/ThemesFrame/common.css" />
    @Scripts.Render("~/bundles/modernizr")
</head>
<body style="background-color:#393D49">
    <style>
        .box-shadow-1 {
            -webkit-box-shadow: 3px 3px 10px;
            -moz-box-shadow: 3px 3px 10px;
            box-shadow: 3px 3px 10px;
        }

        .box-pull-right {
            position: absolute;
            top: 83%;
            right: 10px;
        }

        .row-tip {
            position: absolute;
            left: -50px;
            top: 0;
            height: 100%;
            width: 40px;
            margin-top: 4px;
            margin-bottom: 4px;
            border-right: solid 2px #428bca;
        }
    </style>
    <div class="container">
        <h1 style="font-weight:bold;color:aliceblue">数据资料<small> 2015</small></h1>
        <div style="position:relative">
            <div class="row">
                <div class="col-lg-4 col-md-4 col-sm-4 breadcrumb box-shadow-1" style="height:200px;border:solid 10px #393D49;">
                    <h2>民营经济增加值</h2>
                    <table class="table table-striped ">
                        <tr>
                            <td>民营经济增加值</td>
                            <td>4823.76</td>
                        </tr>
                        <tr>
                            <td>同期增长</td>
                            <td>16.4</td>
                        </tr>
                        <tr>
                            <td>生产总值比重</td>
                            <td>45.2</td>
                        </tr>
                        <tr></tr>
                    </table>
                    <p class="pull-right box-pull-right"><a class="btn btn-primary btn-sm" target="_blank" href="/Data/Increase?year=2015">详情进入 &raquo;</a></p>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-4 breadcrumb box-shadow-1" style="height:200px;border:solid 10px #393D49">
