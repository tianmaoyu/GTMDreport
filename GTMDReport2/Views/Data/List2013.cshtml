                        <tr>
                            <td>固定资产原价</td>
                            <td>121172</td>
                        </tr>
                    </table>
                    <p class="pull-right box-pull-right"><a class="btn btn-primary btn-sm" href="/Data/CateringIndustry?year=2013" target="_blank">详情进入 &raquo;</a></p>
                </div>

            </div>

            <div class="row-tip" style="height:100%;width:40px;">
                <h1 style="color:white">行<br>业<br>类<br>型<br>分<br>类</h1>
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/easy-ui")
    @*@RenderSection("scripts", required: false)*@
</body>
</html>