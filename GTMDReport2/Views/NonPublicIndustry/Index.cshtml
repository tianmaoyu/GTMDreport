
@{
    Layout = null;
}
<body>
    <table id="dg-NonPublicIndustry"></table>
    <div id="tbnpi">
        <a href="#" class="easyui-linkbutton" data-options="iconCls:'icon-redo',plain:true,text:'导出'"></a>
        @*<a href="#" class="easyui-linkbutton" data-options="iconCls:'icon-blank',plain:true,text:'导出'"></a>*@
        <div class="pull-right" style="margin-right:40px">
            <input id="name" class="easyui-textbox" data-options="buttonText:'月份',buttonAlign:'left'" style="width:120px">
            <input id="phoneNumber" class="easyui-textbox" data-options="buttonText:'地区',buttonAlign:'left'" style="width:120px;margin-right:2px">
            <a href="#" class="easyui-linkbutton" data-options="iconCls:'icon-search',plain:true,text:'搜索'" onclick="seacherStudent()"></a>
        </div>
    </div>
    <script>
        $(function () {
            $("#dg-NonPublicIndustry").datagrid({
                url: "NonPublicIndustry/GetPagerInfo",
                toolbar: '#tbnpi',
                striped: true,
                collapsible: true,
                width: "100%",
                height: "100%",
                fitColumn: false,
                showFooter: true,
                columns: [[
                    { field: 'ID', title: 'ID', checkbox: true },
                     //

                 
                    { field: 'PrivateEconomy', title: '民营经济', width: 100 },
                    { field: 'CapitalEconomy', title: '#私  营', width: 100 },
                    { field: 'GrowthRate', title: '同比增长', width: 40 },
                     //学生表的信息
                    { field: 'Date', title: '月份', width: 80 },
                
                    { field: 'IndexName', title: '指标', width: 100 },

                    { field: 'RegionName', title: '地区', width: 80 },
                  
                ]],
                pagination: true,

            });
        })
    </script>
</body>
