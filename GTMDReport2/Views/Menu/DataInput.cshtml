@{
    Layout = null;
}
<style>
    .panel-with-icon {
        padding-left: 3em;
    }
</style>
<div class="easyui-layout" data-options="fit:true,border:false">
    <div data-options="region:'west',title:'录入',iconCls:'icon-data-input',split:true" style="width:180px;">
        <div class="easyui-accordion" data-options="fit:true,border:false">

            <div title="数据录入" data-options="iconCls:'icon-folder'" @*style="padding:10px;"*@>
                <div class="list-group">
                    <a class="list-group-item" url="/NonPublicIndustryUpload/Index" href="#">工业主要指标[上传]<span class="glyphicon glyphicon-upload pull-right"></span></a>
                    <a class="list-group-item" url="/IndustryClassificationUpload/Index" href="#">工业分行业指标[上传]<span class="glyphicon glyphicon-upload pull-right"></span></a>
                </div>
            </div>
           
        </div>
    </div>

    <div data-options="region:'center'" style="padding:5px;background:#eee;">
        <div id="tabs" class="easyui-tabs" data-options="fit:true,border:false" style="width:100%;height:99%;">
            <div title="首页" style="padding:20px;display:none;">

            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        $(".list-group .list-group-item").on("click", function () {
            var title = $(this).text()
            var id = $(this).attr("id");
            var href = $(this).attr("url")
            var tab = $("#tabs").tabs('getTab', title);
            //var content = '<iframe scrolling="no" frameborder="0"  src="' + href + '" style="width:100%;height:100%;"></iframe>';
            if (tab == undefined) {
                $("#tabs").tabs('add', {
                    closable: true,
                    title: title,
                    href: href,
                    //content: content,
                })
            }
            else {
                $("#tabs").tabs('select', title);
            }
        });
    });
</script>
